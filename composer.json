{
    "$schema": "https://getcomposer.org/schema.json",
    "name": "zaplish/core",
    "description": "Zaplish CMS Core Package â€“ admin panel, content engine, and APIs.",
    "type": "library",
    "license": "proprietary",
    "keywords": ["zaplish", "cms", "laravel", "content-management"],
    "require": {
        "php": "^8.2",
        "laravel/framework": "^12.0",
        "intervention/image": "^3.7",
        "sqids/sqids": "^0.5.0"
    },
    "autoload": {
        "psr-4": {
            "Zaplish\\Core\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Zaplish\\Core\\ZaplishServiceProvider"
            ]
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
